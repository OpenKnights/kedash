{
  "name": "kedash",
  "version": "0.3.0",
  "description": "Functional utility library - modern, simple, typed, powerful",
  "type": "module",
  "keywords": [
    "util",
    "kedash"
  ],
  "license": "MIT",
  "homepage": "https://github.com/OpenKnights/kedash#readme",
  "bugs": {
    "url": "https://github.com/OpenKnights/kedash/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenKnights/kedash.git"
  },
  "author": "king3 <w2196592083@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:play": "tsdown cli/play.ts -d cli/.output/ -f esm",
    "play": "node cli/.output/play.js",
    "test": "vitest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "release": "bumpp && pnpm publish",
    "release:beta": "bumpp && pnpm publish --tag=beta"
  },
  "devDependencies": {
    "@king-3/eslint-config": "1.0.8",
    "@king-3/prettier-config": "^1.0.2",
    "@types/node": "^24.9.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
